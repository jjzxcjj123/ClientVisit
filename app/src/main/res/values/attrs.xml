<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleWaveView">
        <attr name="waveColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="waveCount" format="integer" />
    </declare-styleable>

    <declare-styleable name="WaterWaveView">
        <attr name="waveLength" format="dimension"/>
        <attr name="amplitude" format="dimension"/>
        <attr name="waterColor" format="color" />
        <attr name="duration" format="integer"/>
    </declare-styleable>


</resources>